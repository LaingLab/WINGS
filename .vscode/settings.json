{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordWrap": "on",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".vscode": true,
    "build": true,
    "node_modules": true,
    "out": true,
    ".editorconfig": true,
    ".npmrc": true,
    ".prettierignore": true,
    ".prettierrc.yaml": true,
    "eslint.config.mjs": true,
    "pnpm-lock.yaml": true,
    ".gitignore": true,
    "electron-builder.yml": true,
    "tsconfig.json": true,
    "package.json": true,
    "electron.vite.config.ts": true,
    "tsconfig.node.json": true,
    "tsconfig.web.json": true
  },
  "explorerExclude.backup": {},
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "COBOL": true,
    "COBOLIT": true,
    "ACUCOBOL": true,
    "BITLANG-COBOL": true,
    "RMCOBOL": true,
    "COBOL_MF_LISTFILE": false,
    "COBOL_PCOB_LISTFILE": false,
    "COBOL_ACU_LISTFILE": false,
    "COBOL_MF_PREP": false,
    "mfu": false,
    "utreport": false
  }
}
