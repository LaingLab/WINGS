{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.wordWrap": "on",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".vscode": false,
    "build": false,
    "node_modules": false,
    "out": false,
    ".editorconfig": false,
    ".npmrc": false,
    ".prettierignore": false,
    ".prettierrc.yaml": false,
    "eslint.config.mjs": false,
    "pnpm-lock.yaml": false,
    ".gitignore": false,
    "electron-builder.yml": false,
    "tsconfig.json": false,
    "package.json": false,
    "electron.vite.config.ts": false,
    "tsconfig.node.json": false,
    "tsconfig.web.json": false
  },
  "explorerExclude.backup": {},
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "COBOL": true,
    "COBOLIT": true,
    "ACUCOBOL": true,
    "BITLANG-COBOL": true,
    "RMCOBOL": true,
    "COBOL_MF_LISTFILE": false,
    "COBOL_PCOB_LISTFILE": false,
    "COBOL_ACU_LISTFILE": false,
    "COBOL_MF_PREP": false,
    "mfu": false,
    "utreport": false
  }
}
